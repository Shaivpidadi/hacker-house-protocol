# Privy Configuration
# Get these from your Privy dashboard at https://console.privy.io/

# Required: Your Privy App ID (found in your Privy dashboard)
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id_here

# Required: Your Privy Client ID (found in your Privy dashboard)  
NEXT_PUBLIC_PRIVY_CLIENT_ID=your_privy_client_id_here

# Required: Your Privy App Secret (found in your Privy dashboard)
PRIVY_APP_SECRET=your_privy_app_secret_here

# Optional: Session Signer ID for advanced wallet features
# NEXT_PUBLIC_SESSION_SIGNER_ID=your_session_signer_id_here

# Optional: Enable debug mode for development
# NEXT_PUBLIC_PRIVY_DEBUG=true

# HHP Protocol Configuration
# Contract address for the deployed HHP smart contract
NEXT_PUBLIC_HHP_ADDRESS=0xYourDeployedAddress

# Chain ID for the network where HHP is deployed
# 421614 = Arbitrum Sepolia (default)
# 42161 = Arbitrum One
# 11155111 = Sepolia
# 1 = Ethereum Mainnet
NEXT_PUBLIC_CHAIN_ID=421614
